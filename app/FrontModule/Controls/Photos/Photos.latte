<div class="att-images" n:if="count($photos)">
	<ul class="reset">
		{foreach $photos as $photo}
			<li><a href="{plink :Front:Photo:large $photo}" class="spc thickbox" rel="[group 1]"><span class="img"><img src="{plink :Front:Photo:small $photo}"/></span></a></li>
		{/foreach}
	</ul>
</div>
<p class="center">Fotograf: <a href="http://pavelkaras.cz" target="_blank">Pavel Karas</a></p>
